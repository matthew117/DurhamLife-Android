<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomRatingBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/custom_ratingbar_full</item>
        <item name="android:indeterminateDrawable">@drawable/custom_ratingbar_full</item>
        <item name="android:minHeight">57dip</item>
        <item name="android:maxHeight">57dip</item>
        <item name="android:thumb">@null</item>
    </style>

    <style name="CustomRatingBar.Indicator">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/custom_ratingbar</item>
        <item name="android:indeterminateDrawable">@drawable/custom_ratingbar</item>
        <item name="android:minHeight">38dip</item>
        <item name="android:maxHeight">38dip</item>
        <item name="android:thumb">@null</item>
        <item name="android:isIndicator">true</item>
    </style>

</resources>